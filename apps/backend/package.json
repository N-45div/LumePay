        "@nestjs/common": "^10.0.0",
        "@nestjs/core": "^10.0.0",
        "@nestjs/config": "^3.1.1",
        "@nestjs/schedule": "^4.0.0",
        "@nestjs/typeorm": "^11.0.0",
        "@pythnetwork/client": "^2.19.0",
        "@solana/web3.js": "^1.87.6",
        "axios": "^1.8.2",
        "class-transformer": "^0.5.1",
        "class-validator": "^0.14.1",
        "dotenv": "^16.4.7",
        "express": "^4.18.2",
        "mongoose": "^8.0.0",
        "pg": "^8.13.1",
