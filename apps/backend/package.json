    },
    "scripts": {
        "build": "tsc -p tsconfig.json",
        "start": "node dist/main.js",
        "start:dev": "ts-node src/main.ts",
        "test": "jest --detectOpenHandles",
        "test:watch": "jest --watch --detectOpenHandles",
        "test:coverage": "jest --coverage --detectOpenHandles",
        "test:debug": "jest --detectOpenHandles --verbose",
        "test:clear": "jest --clearCache && jest --detectOpenHandles",
        "db:start": "docker-compose up -d postgres",
        "db:stop": "docker-compose down",
        "db:migrate": "node scripts/database-migration.js run",
        "db:migrate:create": "node scripts/database-migration.js create",
        "db:migrate:generate": "node scripts/database-migration.js generate",
        "db:migrate:revert": "node scripts/database-migration.js revert",
        "db:migrate:show": "node scripts/database-migration.js show"
    }
